{
  config.vim.autocomplete = {
    enable = true;
    alwaysComplete = true;

    mappings = {
      close = "<C-e>"; # ctrl-e
      complete = "<C-Space>"; # ctrl-space
      confirm = "<CR>"; # return
      next = "<Tab>";
      previous = "<S-Tab>";
      scrollDocsDown = "<C-f>";
      scrollDocsUp = "<C-d>";
    };
  };
}
